source:
(
	test:
	{
		print "success"
	}
)

successor::() // Must be at least a Structor to be used in sequences.

// Should print "success"
source.test

// Should print "success"
source/successor test

larger_source:
(
	nested:
	(
		test:
		{
			print "larger success"
		}
	)
)

// Should print "larger success"
~/larger_source.nested/successor test
